{
    "command": "/usr/sbin/httpd -DFOREGROUND",
    "config_files": [
        {
            "source": "/var/lib/config-data/merged/httpd.conf",
            "dest": "/etc/httpd/conf/httpd.conf",
            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "/var/lib/config-data/merged/ssl.conf",
            "dest": "/etc/httpd/conf.d/ssl.conf",
            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "/var/lib/config-data/tls/certs/*",
            "dest": "/etc/pki/tls/certs/",
            "owner": "root",
            "perm": "0640",
            "optional": true,
            "merge": true
        },
        {
            "source": "/var/lib/config-data/tls/private/*",
            "dest": "/etc/pki/tls/private/",
            "owner": "root",
            "perm": "0600",
            "optional": true,
            "merge": true
        },
        {
            "source": "/var/lib/config-data/merged/httpd_custom_*",
            "dest": "/etc/httpd/conf/",
            "owner": "apache",
            "perm": "0444",
            "optional": true
        }
    ],
    "permissions": [
        {
            "path": "/var/log/ironic-inspector",
            "owner": "ironic-inspector:ironic-inspector",
            "recurse": true
        },
        {
            "path": "/var/lib/ironic-inspector",
            "owner": "ironic-inspector:ironic-inspector",
            "recurse": true
        }
    ]
}
